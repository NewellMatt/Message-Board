{{#each sortedLikes as |answer|}}
  Author: {{answer.author}}
  Answer: {{answer.content}}

  <button class="btn btn-primary btn-sm" {{action 'upVote' answer}}> Upvote</button>
  <button class="btn btn-primary btn-sm" {{action 'delete' answer}}>Delete</button>

  UpVotes: {{answer.upVotes}}
  <br>
{{/each}}
